package sesliHarfler;

public class sesliHarfler {

	public static void main(String[] args) {
		char harf='C';
		
		switch(harf) {
		case 'A':
		case 'I':
		case 'O':
		case 'U':
			System.out.println("Bu harf kalýn sesli harftir.");
			break;
		case 'E':
		case 'Ý':
		case 'Ö':
		case 'Ü':
			System.out.println("Bu harf ince sesli harftir.");
			break;
			default:
				System.out.println("Bu harf sesli harf deðildir.");
		}
		
	}

}
