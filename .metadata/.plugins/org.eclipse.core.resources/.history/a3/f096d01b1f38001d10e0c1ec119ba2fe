package conditionals;

public class conditionals {

	public static void main(String[] args) {
		int sayi=21;
		if(sayi<20) {
			System.out.println("sayi 20den küçüktür.");
		}
		
		else if(sayi==20) {
			System.out.println("sayi 20'ye eþittir");
		}
		else {
			System.out.println("sayi 20den buyuktur.");
		}
			
	}

}
