package sayiBulma;

public class sayiBulma {

	public static void main(String[] args) {
		int[] sayilar = new int [] {1,2,5,7,9,0};
		int aranacak = 5;
		int i;
		for(i=0;i<sayilar.length;i++) {
			if(sayilar[i]==aranacak) {
				System.out.println("bu sayi dizi içerisinde mevcuttur.");
			}
		}
		
	}

}
