package loopDemo;

public class loopDemo {

	public static void main(String[] args) {
		int i;
		System.out.println("1den 9a kadar olan sayýlarýn yazýmý:");
		for(i=1;i<10;i++) {
			System.out.println(i);
		}
		System.out.println("10a kadar bulunan çift rakamlar:");
		for(i=2;i<10;i+=2) {
			System.out.println(i);
		}
		System.out.println("for döngüsü bitti.");
		//while
		
		while (i<10) {
			System.out.println(i);
			i++;
		}
	}

}
